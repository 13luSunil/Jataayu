<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.org.projecteka.featuresprovider">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="in.org.projecteka.jataayu.JataayuApp"
        android:label="Espresso Test Runner"
        android:theme="@style/Theme.AppCompat.Light">
        <activity
            android:name="in.org.projecteka.jataayu.provider.ui.activity.TestsOnlyActivity"
            android:screenOrientation="userPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>